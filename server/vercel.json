{
	"version": 2,
	"env": {
		"MONGO_URI": "@shop_api_mongo_uri",
		"PORT": "7000",
		"JWT_KEY": "SECRET"
	},
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"methods": [
				"GET",
				"POST",
				"PUT",
				"DELETE"
			]
		}
	]
}